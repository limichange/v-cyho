{
  "name": "vue-pay-password",
  "version": "0.0.3",
  "description": "",
  "main": "dist/vue-pay-password.common.js",
  "unpkg": "dist/vue-pay-password.umd.min.js",
  "jsdelivr": "dist/vue-pay-password.umd.min.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "dev": "vue serve examples/default.vue",
    "build": "vue build src/index.js --target lib",
    "fuck": "rm -rf node_modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limichange/vue-pay-password.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "limichange<limichange@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limichange/vue-pay-password/issues"
  },
  "homepage": "https://github.com/limichange/vue-pay-password#readme",
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/cli": "^3.0.0-beta.1",
    "@vue/cli-plugin-babel": "^3.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.1",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "@vue/eslint-config-standard": "^3.0.0-beta.1",
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "vue-jest": "^1.4.0",
    "vue-server-renderer": "^2.5.13",
    "vue-template-compiler": "^2.5.13"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
